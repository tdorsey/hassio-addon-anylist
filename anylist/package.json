{
    "name": "anylist",
    "version": "1.0.0",
    "description": "Anylist",
    "author": "kevdliu",
    "main": "index.js",
    "dependencies": {
        "anylist": "0.8.5",
        "express": "4.18.2",
        "minimist": "1.2.8"
    },
    "devDependencies": {
        "@faker-js/faker": "^9.9.0",
        "jest": "^29.5.0",
        "msw": "^2.10.3",
        "supertest": "^6.3.3"
    },
    "scripts": {
        "start": "node index.js",
        "test": "jest",
        "test:watch": "jest --watch"
    },
    "jest": {
        "testEnvironment": "node",
        "testMatch": [
            "**/__tests__/**/*.test.js"
        ],
        "setupFilesAfterEnv": [
            "<rootDir>/__tests__/setup.js"
        ],
        "collectCoverageFrom": [
            "index.js"
        ],
        "testTimeout": 10000,
        "verbose": true
    }
}
